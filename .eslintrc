{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "quotes": [2, "single", "avoid-escape"],
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jasmine": true,
    "jest": true
  }
}
